#keystate_numlock = enable
#keystate_scrolllock = enable
#keystate_capslock = enable



"sudo asus-kbd-backlight off"
    m:0x0 + c:237
    XF86KbdBrightnessDown 

"sudo asus-kbd-backlight max"
    m:0x0 + c:238
    XF86KbdBrightnessUp 

"xbacklight -5 -steps 1 -time 0"
    m:0x0 + c:232
    XF86MonBrightnessDown 

"xbacklight +5 -steps 1 -time 0"
    m:0x0 + c:233
    XF86MonBrightnessUp 

"touchpadtoggle"
    m:0x0 + c:199
        XF86TouchpadToggle

"pulseaudio-ctl mute"
    m:0x0 + c:121
    XF86AudioMute

"pulseaudio-ctl down"
    m:0x0 + c:122
    XF86AudioLowerVolume

"pulseaudio-ctl up"
    m:0x0 + c:123
    XF86AudioRaiseVolume
